  },
  "bundle": {
    "active": true,
    "targets": [
      "appimage",
      "deb",
      "rpm"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png", 
      "icons/128x128@2x.png",
      "icons/icon.png"
