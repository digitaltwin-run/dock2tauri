{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devPath": "http://localhost:8088",
    "distDir": "../app"
  },
  "package": {
    "productName": "Dock2Tauri - test",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "icon": [],
      "identifier": "com.dock2tauri.testlatest",
      "targets": [
        "appimage",
        "deb",
        "rpm"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Dock2Tauri - test:latest",
        "width": 1200,
        "height": 800,
        "minWidth": 600,
        "minHeight": 400,
        "resizable": true,
        "fullscreen": false
      }
    ]
  }
}