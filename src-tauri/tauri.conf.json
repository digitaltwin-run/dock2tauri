{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "distDir": "../app",
    "devPath": "http://localhost:8088"
  },
  "package": {
    "productName": "Dock2Tauri - nginx",
    "version": "1.0.0"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "identifier": "com.dock2tauri.nginxalpine",
      "publisher": "Dock2Tauri",
      "createUpdaterArtifacts": false,
      "copyright": "Copyright (c) 2025",
      "category": "DeveloperTool",
      "shortDescription": "Docker App in Tauri",
      "longDescription": "Running nginx:alpine as desktop application"
    },
    "windows": [
      {
        "title": "Dock2Tauri - nginx:alpine",
        "width": 1200,
        "height": 800,
        "minWidth": 600,
        "minHeight": 400,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {}
}