{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "productName": "Dock2Tauri - grafanagrafana",
  "version": "1.0.0",
  "identifier": "com.dock2tauri.grafanagrafana",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devUrl": "http://localhost:3001",
    "frontendDist": "../app"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Dock2Tauri - grafana/grafana",
